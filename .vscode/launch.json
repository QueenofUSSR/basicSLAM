    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch testFeature",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/tests/testFeature",
            "args": ["${workspaceFolder}/datasets/MH01/mav0/cam0/data"],
            "cwd": "${workspaceFolder}/build/tests",
            "stopAtEntry": false,
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Project"
        },
